{
  "name": "@huchenme/github-trending",
  "version": "0.0.0-development",
  "description": "Get GitHub trending data",
  "sideEffects": false,
  "main": "dist/github-trending.cjs.js",
  "module": "dist/github-trending.esm.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huchenme/github-trending-api.git"
  },
  "keywords": [
    "github",
    "trending",
    "api",
    "node"
  ],
  "author": "Hu Chen <chen@huchen.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huchenme/github-trending-api/issues",
    "email": "chen@huchen.dev"
  },
  "homepage": "https://github.com/huchenme/github-trending-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-cache-controller": "^1.1.0",
    "lodash": "^4.17.15",
    "memory-cache": "^0.2.0",
    "node-fetch": "^2.6.0",
    "opencollective-postinstall": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "doctoc": "^1.4.0",
    "eslint": "^6.6.0",
    "eslint-config-kentcdodds": "14.7.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "semantic-release": "^15.13.30",
    "travis-deploy-once": "^5.0.11"
  },
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/server.js",
    "test": "jest --coverage",
    "eslint": "eslint .",
    "validate": "npm-run-all eslint test",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production rollup -c",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "start": "node ./dist/server.cjs.js",
    "postinstall": "opencollective-postinstall || true"
  },
  "lint-staged": {
    "README.md": [
      "doctoc",
      "prettier --write",
      "git add"
    ],
    "**/!(README).+(md|js|json)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn validate && lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/github-trending-api"
  }
}
